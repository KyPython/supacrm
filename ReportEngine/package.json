{
  "name": "reportengine",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:backend": "cd packages/backend && npm run dev",
    "dev:frontend": "cd packages/frontend && npm run dev",
    "dev": "concurrently \"cd packages/backend && npm run dev\" \"cd packages/frontend && npm run dev\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "faker": "^5.5.3"
  }
}
